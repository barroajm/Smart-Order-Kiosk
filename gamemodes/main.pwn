		}
		else
		{
	    	strcpy(adminMOTD, newval, 128);
	    	SAM(COLOR_LIGHTRED, "| TIR:ADMIN | %s has set the admin MOTD to '%s'", GetRPName(playerid), adminMOTD);
		}
	}
	if(!strcmp(option, "helper", true))
	{
 		if(!strcmp(newval, "none", true))
		{
	    	helperMOTD[0] = 0;
	    	SAM(COLOR_LIGHTRED, "| TIR:ADMIN | %s has reset the helper MOTD.", GetRPName(playerid));
		}
		else
		{
	    	strcpy(helperMOTD, newval, 128);
	    	SAM(COLOR_LIGHTRED, "| TIR:ADMIN | %s has set the helper MOTD to '%s'", GetRPName(playerid), helperMOTD);
		}
	}

	SaveServerInfo();
	return 1;
}


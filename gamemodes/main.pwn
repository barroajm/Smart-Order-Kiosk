			TogglePlayerControllable(playerid, 0);
			SetTimerEx("ComservEx", 10000, false, "i", playerid);
			PlayerInfo[playerid][pLastClean] = gettime();
			return 1;
		}
	}
	SCM(playerid, COLOR_GREY2, "You are not in range of cleaning areas.");

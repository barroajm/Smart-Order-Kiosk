{
	SCM(playerid, COLOR_LIGHTORANGE, "** BUSINESS: /buybiz, /lock, /bwithdraw, /bdeposit, /sellbiz, /sellmybiz, /bizinfo.");
	SCM(playerid, COLOR_LIGHTORANGE, "** BUSINESS: /entryfee, /businessmenu, /products.");

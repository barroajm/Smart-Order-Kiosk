						}
					}
			    }
			    else if(response == EDIT_RESPONSE_CANCEL)
			    {
     			    if(PlayerInfo[playerid][pMenuType] == 0)
						ShowObjectSelectionMenu(playerid, MODEL_SELECTION_LANDOBJECTS);
